{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "dev": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "http://localhost:5888",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DB_CONNECTION_STRING": "Host=HOST_IP;Database=DB_NAME;Username=USERNAME;Password=PASSWORD"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "https://localhost:5888",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production",
        "DB_CONNECTION_STRING": "Host=HOST_IP;Database=DB_NAME;Username=USERNAME;Password=PASSWORD"
      }
    }
  }
}
